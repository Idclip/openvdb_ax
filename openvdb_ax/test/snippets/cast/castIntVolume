// Same as castInt without short data.  Used for volume tests since "short" volumes
// are not supported at the moment.  Can be deprecated if this ever changes

int@int_test = 1;

int@int_test2 = int(int@int_test);
long@long_test = long(int@int_test);

float@float_test = float(int@int_test);
double@double_test = double(int@int_test);


