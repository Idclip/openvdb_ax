
// float test locals

float a = 0.0f;
float b = 5.0f;
float c = 0.0f;

a=b=c;

@float_test1 = a;
@float_test2 = b;
@float_test3 = c;

// test float attribs

@float_test4;
@float_test5;
@float_test6 = 10.0f;

@float_test4=@float_test5=@float_test6;


// test vec3f locals

vec3f d = { 4.0f, -3.0f, 1.0f };
vec3f e;
vec3f f = { 0.0f, 0.0f, 0.0f };

d=e=f;

v@vec_float_test1 = d;
v@vec_float_test2 = e;
v@vec_float_test3 = f;


// test vec3f attribs

v@vec_float_test4;
v@vec_float_test5 = { 0.0f, -3.0f, 0.0f };
v@vec_float_test6 = { 1.0f, 0.0f, 0.0f };

v@vec_float_test4=v@vec_float_test5=v@vec_float_test6=v@vec_float_test5;
